[#ftl]
[@b.head/]
[@b.grid items=graduates var="graduate"]
  [@b.row]
    [@b.boxcol /]
    [@b.col width="7%" property="std.code" title="学号"/]
    [@b.col width="8%" property="std.name" title="姓名"/]
    [@b.col width="7%" property="std.state.grade" title="年级"/]
    [@b.col width="8%" property="std.stdType.name" title="学生类别"/]
    [@b.col width="15%" property="std.state.department.name" title="院系"/]
    [@b.col width="10%" property="graduateOn" title="毕业时间"/]
    [@b.col width="10%" property="result.name" title="毕结业情况"/]
    [@b.col width="10%" title="成绩单"]
      [@b.a href="!download?graduate.id=${graduate.id}&docType=transcript" target="_blank"]下载[/@]
    [/@]
    [@b.col width="10%" title="英文成绩单"]
      [@b.a href="!download?graduate.id=${graduate.id}&docType=transcript_en" target="_blank"]下载[/@]
    [/@]
    [@b.col width="10%" title="学位翻译"]
      [#if graduate.degree??]
        [@b.a href="!download?graduate.id=${graduate.id}&docType=degree" target="_blank"]下载[/@]
      [/#if]
    [/@]
    [@b.col width="10%" title="学历翻译"]
      [#if graduate.certificateNo??]
        [@b.a href="!download?graduate.id=${graduate.id}&docType=graduate" target="_blank"]下载[/@]
      [/#if]
    [/@]
  [/@]
[/@]
[@b.foot/]
